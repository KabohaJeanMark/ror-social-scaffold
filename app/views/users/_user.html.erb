<% if current_user!= user%>
  <li>
    Name: <%= user.name %>
    <span class="profile-link">
      <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
    </span>
    <span>
      <%= link_to 'Add Friend', friendships_path(friendship: {user_id:current_user.id, friend_id:user.id, confirmed:false}), method: :post, class: 'profile-link' %>
    </span>
  </li>
<% end %>